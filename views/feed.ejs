<% include ./partials/header.ejs%>
    <div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
        <div class="cards flex flex-wrap gap-10">
            <% posts.forEach(function(elem){ %>
                <div class="card w-32">
                    <div class="w-full bg-sky-200 rounded-lg overflow-hidden">
                        <a href="/show/posts/<%= elem._id%>"><img id="<%= elem._id%>" src="/images/uploads/<%= elem.image%>" alt=""></a>
                    </div>
                    <h5 class="mt-2 text-sm"><%= elem.title%></h5>
                </div>
            <%})%>
        </div>
    </div>
<% include ./partials/footer.ejs%>